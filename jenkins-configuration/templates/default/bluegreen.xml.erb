<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>blue</name>
          <description>This refers to the current production environment's IP address. After this job is run the IP you enter here will no longer be the production environment. eg. 54.212.215.115</description>
          <defaultValue></defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>green</name>
          <description>This refers to the intended production environment's IP address. After this job is run, the IP you enter here will be the new production environment. eg. 64.211.245.125</description>
          <defaultValue></defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>subdomain</name>
          <description>This refers to the A record that you like to connect to. If you enter app, you will be connecting the IP to app.hostedzone.com</description>
          <defaultValue></defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>hostedzone</name>
          <description>This refers to the Route53 zone that you would like to connect to. If you enter stelligentlabs.com, you'll be connecting your IP to app.stelligentlabs.com</description>
          <defaultValue></defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>ruby bin/route53switch.rb --blue ${blue} --green ${green} --subdomain ${subdomain} --hostedzone ${hostedzone}</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>
